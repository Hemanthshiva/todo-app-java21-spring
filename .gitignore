*.class
*.jar
*.war
*.ear

# IntelliJ IDEA
.idea/
*.iml
*.iws
*.ipr

# VS Code
.vscode/

# Eclipse / STS
.classpath
.project
.settings/
.apt_generated/
.factorypath
.sts4-cache/

